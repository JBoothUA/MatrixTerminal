  <!-- Level Up! = commit, push, and create pull request -->
  <Snippet>
    <Header>
      <Title>Level Up</Title>
      <Description>commit, push, and create pull request</Description>
      <Author Url="https://www.jessebooth.com" Email="jesse@matrix.tools">JB</Author>
      <Version>1.0</Version>
      <ShellTypes>
        <ShellType>cmd</ShellType>
        <ShellType>powershell</ShellType>
      </ShellTypes>
    </Header>
    <Declarations>
      <Literal>
        <ID>Comment</ID>
        <ToolTip>The pull request comment.</ToolTip>
        <Default>Save Point</Default>
      </Literal>
      <Literal>
        <ID>Repo</ID>
        <ToolTip>The repository to update.</ToolTip>
        <Default>MatrixToolsCore</Default>
      </Literal>
    </Declarations>
    <Code Delimiter="$">
      <![CDATA[
    $currentBranch = git branch --show-current
    az repos pr create --title "$Comment$" --organization https://dev.azure.com/MatrixTools --project MatrixTools -r $Repo$ -t master -s $currentBranch --open
]]>        <!-- return of line, the command is launched after clicking OK -->
    </Code>
  </Snippet>


